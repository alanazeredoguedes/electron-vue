  "main": "dist-electron/main/index.js",
  "description": "Vue Electron",
  "author": "Alan",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
